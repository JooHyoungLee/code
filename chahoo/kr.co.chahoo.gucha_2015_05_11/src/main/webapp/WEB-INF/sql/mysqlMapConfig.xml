		<typeAlias type="kr.co.chahoo.file.model.FileDto" alias="fileDto"/>	
		
		<typeAlias type="kr.co.chahoo.member.model.MemberDto" alias="memberDto"/>	
		<typeAlias type="kr.co.chahoo.member.model.GroupDto" alias="groupDto"/>	
		<typeAlias type="kr.co.chahoo.member.model.GradeDto" alias="gradeDto"/>	
		
		<typeAlias type="kr.co.chahoo.security.CustomUserDetails" alias="customUserDetails"/>	
		
		<typeAlias type="kr.co.chahoo.event.model.EventDto" alias="eventDto"/>	
		
	</typeAliases>
		
